{
  "name": "kkc",
  "displayName": "Knorks Keyboard Config",
  "publisher": "Knork",
  "icon": "images/avatar.png",
  "description": "",
  "version": "0.0.12",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Other"
  ],
  "resources": [
    {
      "pattern": "images/**"
    }
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "kckc.modeOn",
        "title": "modeOn"
      },
      {
        "command": "kckc.modeOff",
        "title": "modeOff"
      },
      {
        "command": "kckc.modeToggle",
        "title": "modeToggle"
      },
      {
        "command": "kckc.selectActionReset",
        "title": "..."
      },
      {
        "command": "kckc.selectAction",
        "title": "..."
      },
      {
        "command": "kckc.selectActionResetAction",
        "title": "..."
      },
      {
        "command": "kckc.modAllSelections",
        "title": "..."
      },
      {
        "command": "kckc.makeTempSelectionActive",
        "title": "..."
      },
      {
        "command": "kckc.setShiftHat",
        "title": "..."
      },
      {
        "command": "kckc.clearSelection",
        "title": "..."
      },
      {
        "command": "kckc.setUserMode",
        "title": "..."
      },
      {
        "command": "kckc.setHat",
        "title": "..."
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/heap": "^0.2.34",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.88.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "heap": "^0.2.7"
  }
}
